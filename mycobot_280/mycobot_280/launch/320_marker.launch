    <arg name="model" default="$(find mycobot_description)/urdf/new_320_urdf/new_mycobot_pro_320.urdf"/>
	<arg name="rvizconfig" default="$(find new_mycobot_320)/config/mycobot_320_with_marker.rviz" />
